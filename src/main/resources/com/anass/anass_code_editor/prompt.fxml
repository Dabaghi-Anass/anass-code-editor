         <font>
            <Font name="Consolas Bold" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="commandField" prefHeight="26.0" prefWidth="620.0" style="-fx-padding: 0; -fx-background-color: transparent; -fx-text-fill: white; -fx-border-width: 0;" HBox.hgrow="SOMETIMES">
